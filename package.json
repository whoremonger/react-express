{
  "name": "react-express",
  "version": "1.0.0",
  "description": "A tutorial of how to connect express (the backend) to react (the frontend).",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Jarrad L.",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.2.2",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16"
  }
}
